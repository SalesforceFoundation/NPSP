    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Committed</fullName>
                <default>true</default>
                <label>Committed</label>
            </value>
            <value>
                <fullName>Finalized</fullName>
                <default>false</default>
                <label>Finalized</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
