    <fields>
        <fullName>Amount_to_Goal__c</fullName>
        <description>Formula field showing the Amount left towards the Campaign&apos;s Expected Revenue</description>
        <externalId>false</externalId>
        <formula>IF(
            NOT(ISBLANK(ExpectedRevenue)),
            ExpectedRevenue - AmountWonOpportunities,
            0
            )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Formula field showing the Amount left towards the Campaign&apos;s Expected Revenue</inlineHelpText>
        <label>Amount to Goal</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
    <listViews>
